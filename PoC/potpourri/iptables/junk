-P INPUT ACCEPT
-P FORWARD ACCEPT
-P OUTPUT ACCEPT
-A INPUT -i testbrd -p udp -m udp --dport 53 -j ACCEPT
-A INPUT -i testbrd -p tcp -m tcp --dport 53 -j ACCEPT
-A INPUT -i testbrd -p udp -m udp --dport 67 -j ACCEPT
-A INPUT -i testbrd -p tcp -m tcp --dport 67 -j ACCEPT
-A FORWARD -d 10.11.10.0/24 -o testbrd -j ACCEPT
-A FORWARD -s 10.11.10.0/24 -i testbrd -j ACCEPT
-A FORWARD -i testbrd -o testbrd -j ACCEPT
-A FORWARD -o testbrd -j REJECT --reject-with icmp-port-unreachable
-A FORWARD -i testbrd -j REJECT --reject-with icmp-port-unreachable
-A OUTPUT -o testbrd -p udp -m udp --dport 68 -j ACCEPT
